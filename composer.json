{
  "name": "motionpanel/motionpanel-laravel",
  "version": "0.0.5",
  "description": "A beautiful dashboard to monitor your laravel application",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "dashboard",
    "monitoring",
    "motionpanel",
    "motionpanel-laravel",
    "grafana",
    "prometheus",
    "node-exporter",
    "laravel"
  ],
  "autoload": {
    "psr-4": {
      "MotionPanel\\MotionPanelLaravel\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MotionPanel\\MotionPanelLaravel\\Tests\\": "tests/",
      "Workbench\\App\\": "workbench/app/",
      "Workbench\\Database\\Factories\\": "workbench/database/factories/",
      "Workbench\\Database\\Seeders\\": "workbench/database/seeders/"
    }
  },
  "authors": [
    {
      "name": "Tony Soekirman",
      "email": "tonywei92@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.0",
    "laravel/framework": "^9.0|^10.0|^11.0",
    "illuminate/support": "^9.0|^10.0|^11.0",
    "illuminate/http": "^9.0|^10.0|^11.0",
    "illuminate/database": "^9.0|^10.0|^11.0"
  },
  "require-dev": {
    "orchestra/testbench": "^9.5",
    "pestphp/pest": "^3.3",
    "pestphp/pest-plugin-laravel": "^3.0",
    "phpstan/phpstan": "^1.12"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    },
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "analyse": "vendor/bin/phpstan analyse src tests",
    "baseline": "vendor/bin/phpstan analyse src tests --generate-baseline",
    "test": "vendor/bin/pest",
    "lint": [
      "@php vendor/bin/phpstan analyse --verbose --ansi"
    ],
    "post-autoload-dump": [
      "@clear",
      "@prepare"
    ],
    "clear": "@php vendor/bin/testbench package:purge-skeleton --ansi",
    "prepare": "@php vendor/bin/testbench package:discover --ansi",
    "build": "@php vendor/bin/testbench workbench:build --ansi",
    "serve": [
      "Composer\\Config::disableProcessTimeout",
      "@build",
      "@php vendor/bin/testbench serve --ansi"
    ]
  }
}
